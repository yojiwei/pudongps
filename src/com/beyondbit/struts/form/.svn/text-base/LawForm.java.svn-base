package com.beyondbit.struts.form;

import org.apache.struts.action.ActionForm;

public class LawForm extends ActionForm{
    private String id;
    //权力标准id
    private String psid;
    //权力子标准id
    private String spsid;
    //权力依据名称
    private String lawname;
    //权力依据类型
    private String bykind;
    //制定机关
    private String consteorgan;
    //制定日期
    private String constedate;
    //依据内容
    private String describe;

    public LawForm() {
    }
    public String getBykind() {
        return bykind;
    }
    public String getConstedate() {
        return constedate;
    }
    public String getConsteorgan() {
        return consteorgan;
    }
    public String getDescribe() {
        return describe;
    }
    public String getLawname() {
        return lawname;
    }
    public void setBykind(String bykind) {
        this.bykind = bykind;
    }
    public void setConstedate(String constedate) {
        this.constedate = constedate;
    }
    public void setConsteorgan(String consteorgan) {
        this.consteorgan = consteorgan;
    }
    public void setDescribe(String describe) {
        this.describe = describe;
    }
    public void setLawname(String lawname) {
        this.lawname = lawname;
    }
    public String getId() {
        return id;
    }
    public void setId(String id) {
        this.id = id;
    }
    public String getSpsid() {
        return spsid;
    }
    public void setSpsid(String spsid) {
        this.spsid = spsid;
    }
    public String getPsid() {
        return psid;
    }
    public void setPsid(String psid) {
        this.psid = psid;
    }

}
