<class 
    name="com.beyondbit.hibernate.TbQlgkPscategory" 
    table="TB_QLGK_PSCATEGORY"
>

    <id
        name="cateid"
        type="java.lang.Integer"
        column="CATEID"
    >
        <generator class="assigned" />
    </id>

    <property
        name="catecode"
        type="java.lang.String"
        column="CATECODE"
        not-null="true"
        length="20"
    />
    <property
        name="categoryname"
        type="java.lang.String"
        column="CATEGORYNAME"
        not-null="true"
        length="50"
    />
    <property
        name="pcateid"
        type="java.math.BigDecimal"
        column="PCATEID"
        length="22"
    />
    <property
        name="dsc"
        type="java.lang.String"
        column="DSC"
        length="100"
    />
    <property
        name="sequence"
        type="java.math.BigDecimal"
        column="SEQUENCE"
        length="22"
    />

