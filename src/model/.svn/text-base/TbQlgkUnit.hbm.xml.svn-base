        type="java.lang.String"
        column="ADDRESS"
        length="80"
    />
    <property
        name="linkman"
        type="java.lang.String"
        column="LINKMAN"
        length="20"
    />
    <property
        name="type"
        type="java.lang.String"
        column="TYPE"
        length="20"
    />
    <property
        name="groupname"
        type="java.lang.String"
        column="GROUPNAME"
        length="200"
    />
    <property
        name="punitid"
        type="java.math.BigDecimal"
        column="PUNITID"
        length="22"
    />
    <property
        name="sortorder"
