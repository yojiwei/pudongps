<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
    
    add by zhanglun 2009-2-20
-->

<class 
    name="com.beyondbit.hibernate.TbQlgkCBUnit" 
    table="TB_QLGK_CBUNIT"
>

    <id
        name="id"
        type="java.lang.Integer"
        column="ID"
    >
        <generator class="increment" />
    </id>
    <property
        name="cbid"
        type="java.lang.String"
        column="CBID"
    />
    <property
        name="subid"
        type="java.lang.Integer"
        column="SUBID"
    />
    <property
        name="cb"
        type="java.lang.String"
        column="CB"
        length="100"
    />
  
    <property
        name="bjob"
        type="java.lang.String"
        column="BJOB"
        length="100"
    />
    <property
        name="cbname"
        type="java.lang.String"
        column="CBNAME"
        length="100"
    />
    
    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to TbQlgkSubpowerstd -->
    <many-to-one
        name="subpower"
        class="com.beyondbit.hibernate.TbQlgkSubpowerstd"
        not-null="true" insert="false" update="false"
    >
        <column name="SUBID" />
    </many-to-one>
    <!-- bi-directional one-to-many association to TbQlgkPowerstdstep -->
    <set
        name="cbsteps"
        lazy="false"
        inverse="true"
		cascade="none"
		order-by="stepid asc"
    >
        <key>
            <column name="CBID" />
        </key>
        <one-to-many 
            class="com.beyondbit.hibernate.TbQlgkPowerstdstep"
        />
    </set>

</class>
</hibernate-mapping>
